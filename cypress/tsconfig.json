{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es5",
			"dom"
		],
		"types": [
			"cypress",
			"@testing-library/cypress",
			"./support/commands"
		],
		"baseUrl": ".",
		"jsx": "react-jsx",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"paths": {
			"utils/*": ["utils/*"],
			"src/*": ["../src/*"],
			"test/components/*": ["component/components/*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx"
	]
}